+    // TAMANHO MINIMO DA MENSAGEM
+    // TAMANHO MAXIMO DA MENSAGEM
+    if (size < CMD_TOTAL_SIZE_MIN ||
+        size > CMD_TOTAL_SIZE_MAX)
+        NOTHING_CMD_ERR(INVALID_CMD_SIZE);
+
+    void* const buff = kmalloc(size, GFP_NOWAIT);
+
+    if (buff == NULL)
+        NOTHING_CMD_ERR(ALLOC_CMD);
