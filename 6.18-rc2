+
+#define __link(o, ptr_to_next) { \
+        if ((o->next = (ptr_to_next))) { \
+             o->next->ptr = &o->next; \
+        }  *(o->ptr = &(ptr_to_next)) = o; \
+    }
+
+// NOTE: O->NEXT FICARA INVALIDO
+#define __unlink(o) { \
+        if ((*o->ptr = o->next)) { \
+            (*o->ptr)->ptr = o->ptr; \
+        }     o->ptr = NULL; \
+    }
+
