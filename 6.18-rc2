+                // THE PATH IS OFF AND IDLE
+                ASSERT(!(path->info & ~P_INFO));
+                ASSERT(!(node->kpaths & KPATH(pid)));
+                ASSERT(!(node->ipaths & IPATH(pid)));
+                ASSERT(!(node->opaths & OPATH(pid)));
